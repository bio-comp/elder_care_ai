# This file configures the local Flytekit environment.
# It's used by commands like 'pyflyte run' and 'pyflyte register'.

[platform]
# The address of the Flyte backend API.
# This points to a local flyte-demo cluster.
url=localhost:30080
insecure=true

[project]
# The default project to register workflows under.
project=elder_risk

[domain]
# The default domain for execution.
domain=development

[images]
# Defines the default Docker image containing the project's code.
# This should match the image you build with your Dockerfile.
default=ghcr.io/my-org/elder_risk:0.1.0-dev

[local_execution]
# Path for storing artifacts when running with 'pyflyte run --local'.
path=./flyte_local_output/
